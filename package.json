{
  "name": "node-updown",
  "description": "An API wrapper for updown.io",
  "license": "MIT",
  "author": "Wouter Buckens",
  "repository": {
    "type": "git",
    "url": "https://github.com/woubuc/node-updown"
  },
  "bugs": {
    "url": "https://github.com/woubuc/node-updown/issues"
  },
  "version": "0.2.2",
  "main": "dist/Updown.js",
  "scripts": {
    "build": "rimraf dist/* && tsc",
    "contributors:add": "all-contributors add",
    "contributors:generate": "all-contributors generate",
    "docs": "run-s docs:*",
    "docs:cleanup": "rimraf dist/* docs/*",
    "docs:declarations": "tsc --emitDeclarationOnly --declarationMap false --skipLibCheck",
    "docs:generate": "typedoc --options typedoc.json dist/",
    "lint": "eslint --ext .ts src/**/*",
    "prepack": "run-s test build",
    "test": "run-s lint && jest"
  },
  "dependencies": {
    "node-fetch": "^2.6.0"
  },
  "devDependencies": {
    "@types/dotenv": "^6.1.1",
    "@types/jest": "^24.0.17",
    "@types/node-fetch": "^2.5.0",
    "@typescript-eslint/eslint-plugin": "^2.0.0",
    "@typescript-eslint/parser": "^2.0.0",
    "all-contributors-cli": "^6.8.1",
    "dotenv": "^8.0.0",
    "eslint": "^6.1.0",
    "eslint-plugin-jest": "^22.15.1",
    "eslint-plugin-jsdoc": "^15.8.1",
    "eslint-plugin-unicorn": "^10.0.0",
    "jest": "^24.9.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.0",
    "ts-jest": "^24.0.2",
    "typedoc": "^0.15.0",
    "typescript": "^3.5.3"
  },
  "keywords": [
    "api",
    "promises",
    "rest",
    "updown",
    "updown.io",
    "wrapper"
  ],
  "engines": {
    "node": ">=8"
  }
}
